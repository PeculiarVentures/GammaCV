{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  }, 
  "extends": [
    "airbnb-typescript"
  ],
  "plugins": [
    "jsx-a11y",
    "import",
    "react",
    "react-hooks"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "react/static-property-placement": 0,
    "jsx-a11y/anchor-is-valid": 1,
    "import/no-unresolved": 1
  }
}